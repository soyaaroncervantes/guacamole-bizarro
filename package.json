  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "name": "guacamole-bizarro",
  "private": true,
  "scripts": {
    "ng": "ng",
    "apps:target": "yarn apps:albums:firebase:target; yarn apps:portfolio:firebase:target",
    "apps:build": "yarn apps:albums:build; yarn apps:portfolio:build",
    "apps:deploy": "yarn apps:albums:deploy; yarn apps:portfolio:deploy",
    "apps:portfolio:start": "ng s @apps/portfolio --port 4201",
    "apps:portfolio:build": "ng b @apps/portfolio --configuration production --build-optimizer --optimization",
    "apps:portfolio:test": "ng test @apps/portfolio",
    "apps:portfolio:lint": "ng lint @apps/portfolio",
    "apps:portfolio:e2e": "ng e2e @apps/portfolio",
    "apps:portfolio:deploy": "firebase deploy --only hosting:@apps/portfolio",
    "apps:portfolio:firebase:build": "ng b @apps/portfolio  --configuration production --watch",
    "apps:portfolio:firebase:target": "firebase target:apply hosting @apps/portfolio guacamole-bizarro",
    "apps:albums:start": "ng s @apps/albums --port 4200",
    "apps:albums:build": "ng b @apps/albums --configuration production --build-optimizer --optimization",
    "apps:albums:test": "ng test @apps/albums",
    "apps:albums:lint": "ng lint @apps/albums",
    "apps:albums:e2e": "ng e2e @apps/albums",
    "apps:albums:deploy": "firebase deploy --only hosting:@apps/albums",
    "apps:albums:firebase:build": "ng b @apps/albums --configuration production --watch",
    "apps:albums:firebase:target": "firebase target:apply hosting @apps/albums albums-bizarro",
    "apps:firebase:serve": "firebase serve"
  },
  "version": "0.0.0"
}